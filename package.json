{
  "name": "@markwhen/timeline",
  "version": "1.4.3",
  "scripts": {
    "dev": "vite --port=5176",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port=5176",
    "build-only": "vite build && cp _headers dist/_headers",
    "type-check": "vue-tsc --noEmit",
    "bump": "node ./scripts/bumper.js patch",
    "link:parser": "yalc add @markwhen/parser && yalc link @markwhen/parser && npm install",
    "unlink:parser": "yalc remove @markwhen/parser && npm install"
  },
  "dependencies": {
    "@markwhen/parser": "^0.13.5",
    "@squadette/hammerjs": "^2.1.0-pre3",
    "@vueuse/core": "^10.11.0",
    "@vueuse/integrations": "^10.11.0",
    "immer": "^10.1.1",
    "luxon": "^3.5.0",
    "pinia": "^2.2.2",
    "vue": "^3.5.10",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@types/luxon": "^3.4.2",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/devtools": "^6.6.3",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vite-plugin-singlefile": "^0.13.5",
    "vue-tsc": "^2.1.6"
  },
  "files": [
    "dist/index.html"
  ]
}